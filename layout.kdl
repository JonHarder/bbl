layout {
	pane split_direction="vertical" {
		pane size="20%" {
			plugin location="zellij:strider"
		}
		pane size="80%" {
			pane size="65%" {
				edit "src/main.rs"
				focus true
			}
			pane command="bacon" size="35%" {
				args "--job" "test"
			}
		}
	}
	pane size=1 borderless=true {
		plugin location="zellij:compact-bar"
	}
}
